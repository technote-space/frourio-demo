{
  "name": "@frourio-demo/server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn build:frourio && webpack --mode=production",
    "build:frourio": "yarn migrate:dev && frourio",
    "build:prisma": "prisma generate",
    "build:types": "yarn build:frourio",
    "dev": "run-p dev:*",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "dev:server": "webpack --watch --mode=development",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:init": "prisma migrate dev --name init --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:seed": "yarn ts-node -r tsconfig-paths/register ./prisma/seed.ts",
    "setup": "yarn install --prefer-offline --network-timeout 1000000 && run-s setup:*",
    "setup:config": "npx shx cp -u .env.example ./.env && npx shx cp -u prisma/.env.example prisma/.env",
    "setup:db": "npx shx rm -f prisma/dev.db && yarn migrate:dev",
    "start": "cross-env NODE_ENV=production node index.js",
    "update": "npx npm-check-updates -u --timeout 100000"
  },
  "dependencies": {
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@prisma/client": "^2.16.0",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.13.1",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "fastify": "^3.11.0",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.2.0",
    "fastify-jwt": "^2.3.0",
    "fastify-multipart": "^3.3.1",
    "fastify-static": "^3.4.0",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@prisma/cli": "^2.16.0",
    "@technote-space/material-table": "^2.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/faker": "^5.1.6",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "aspida": "^1.3.0",
    "cross-blob": "^2.0.0",
    "cross-env": "^7.0.3",
    "faker": "^5.2.0",
    "frourio": "^0.22.2",
    "nodemon-webpack-plugin": "^4.4.4",
    "npm-run-all": "^4.1.5",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-node-externals": "^2.5.2"
  },
  "engines": {
    "yarn": "1.x"
  }
}
