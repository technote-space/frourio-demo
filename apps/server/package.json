{
  "name": "@frourio-demo/server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn build:frourio && node --max_old_space_size=2048 --gc_interval=100 node_modules/webpack/bin/webpack.js --mode=production",
    "build:frourio": "yarn migrate:deploy && frourio",
    "build:prisma": "prisma generate",
    "build:types": "yarn build:frourio",
    "dev": "run-p dev:*",
    "dev:frourio": "frourio --watch",
    "dev:prisma": "prisma generate --watch",
    "dev:server": "webpack --watch --mode=development",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:init": "npm_config_yes=true npx shx rm -f prisma/dev.db && npx shx rm -fr prisma/migrations && prisma migrate dev --name init --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:seed": "yarn ts-node -r tsconfig-paths/register ./prisma/seed.ts",
    "setup": "yarn install --prefer-offline --network-timeout 1000000 && run-s setup:*",
    "setup:config": "npm_config_yes=true npx shx cp -u .env.example ./.env && npx shx cp -u prisma/.env.example prisma/.env",
    "setup:db": "npm_config_yes=true npx shx rm -f prisma/dev.db && yarn migrate:dev",
    "start": "cross-env NODE_ENV=production node index.js",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000",
    "log": "cat logs/system.log | yarn bunyan",
    "log:debug": "cat logs/system.log | yarn bunyan -l debug",
    "log:warn": "cat logs/system.log | yarn bunyan -l warn",
    "log:error": "cat logs/system.log | yarn bunyan -l error"
  },
  "dependencies": {
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@prisma/client": "^2.17.0",
    "@slack/webhook": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.15",
    "class-validator": "^0.13.1",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "fastify": "^3.12.0",
    "fastify-cors": "^5.2.0",
    "fastify-helmet": "^5.2.0",
    "fastify-jwt": "^2.3.0",
    "fastify-multipart": "^4.0.0",
    "fastify-static": "^4.0.0",
    "html-to-text": "^7.0.0",
    "node-cron": "^2.0.3",
    "node-fetch": "^2.6.1",
    "nodemailer": "^6.4.18",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "sanitize-html": "^2.3.2",
    "stripe": "^8.137.0",
    "tsyringe": "^4.4.0"
  },
  "devDependencies": {
    "@technote-space/material-table": "^2.3.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bunyan": "^1.8.6",
    "@types/faker": "^5.1.7",
    "@types/html-to-text": "^6.0.0",
    "@types/jest": "^26.0.20",
    "@types/node-cron": "^2.0.3",
    "@types/node-fetch": "^2.5.8",
    "@types/nodemailer": "^6.4.0",
    "@types/qrcode": "^1.4.0",
    "@types/sanitize-html": "^1.27.1",
    "aspida": "^1.5.0",
    "cross-blob": "^2.0.0",
    "cross-env": "^7.0.3",
    "encoding": "^0.1.13",
    "faker": "^5.4.0",
    "frourio": "^0.23.0",
    "nodemon-webpack-plugin": "^4.4.4",
    "npm-run-all": "^4.1.5",
    "prisma": "^2.17.0",
    "raw-loader": "^4.0.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.5",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "engines": {
    "yarn": "1.x"
  }
}
