{
  "name": "@frourio-demo/front",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build && next export",
    "dev": "yarn kill && HOST=$npm_package_host PORT=$npm_package_port NODE_ENV=development yarn ts-node ./server.ts",
    "kill": "lsof -i:$npm_package_port -t | xargs kill -9",
    "setup": "yarn install --prefer-offline --network-timeout 1000000 && yarn setup:ssl",
    "setup:ssl": "run-script-os",
    "setup:ssl:linux:darwin": "yarn setup:hosts && yarn setup:mkcert && yarn setup:cert",
    "setup:ssl:default": "echo 'support only macOS and Linux'",
    "setup:hosts": "if [[ $(cat /etc/hosts | grep $npm_package_host -c) == 0 ]]; then echo \"127.0.0.1 $npm_package_host\" | sudo tee -a /etc/hosts; fi",
    "setup:mkcert": "run-script-os",
    "setup:mkcert:darwin": "brew install mkcert nss",
    "setup:mkcert:linux": "yarn setup:mkcert:linux:brew && yarn setup:mkcert:linux:install",
    "setup:mkcert:linux:brew": "if command -v yum > /dev/null 2>&1; then sudo yum install nss-tools; else sudo apt install libnss3-tools; fi",
    "setup:mkcert:linux:install": "brew install mkcert nss",
    "setup:cert": "mkcert -install && mkcert 'localhost.frourio-demo.com'",
    "start": "yarn kill && HOST=$npm_package_host PORT=$npm_package_port NODE_ENV=production yarn ts-node ./server.ts",
    "update": "npx npm-check-updates -u --timeout 100000 -x next-transpile-modules"
  },
  "dependencies": {
    "@aspida/axios": "^1.4.1",
    "@aspida/swr": "^1.4.1",
    "@auth0/auth0-react": "^1.2.0",
    "@chakra-ui/icons": "^1.0.5",
    "@chakra-ui/react": "^1.3.2",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@fullcalendar/common": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "aspida": "^1.4.1",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.17.0",
    "framer-motion": "^3.3.0",
    "next": "^10.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.0",
    "slick-carousel": "^1.8.1",
    "swr": "^0.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.6.3",
    "@types/node": "^14.14.25",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.1",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-slick": "^0.23.4",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.7",
    "npm-run-all": "^4.1.5",
    "run-script-os": "^1.1.5",
    "ts-loader": "^8.0.15",
    "ts-node": "^9.1.1",
    "typescript": "4.1"
  },
  "engines": {
    "yarn": "1.x"
  },
  "host": "localhost.frourio-demo.com",
  "port": 3001
}
