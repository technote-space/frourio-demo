{
  "name": "@frourio-demo/front",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build && next export",
    "dev": "yarn kill && HOST=$npm_package_host PORT=$npm_package_port NODE_ENV=development yarn ts-node ./server.ts",
    "kill": "lsof -i:$npm_package_port -t | xargs kill -9",
    "setup": "yarn install --prefer-offline --network-timeout 1000000 && yarn setup:ssl",
    "setup:ssl": "run-script-os",
    "setup:ssl:linux:darwin": "yarn setup:hosts && yarn setup:mkcert && yarn setup:cert",
    "setup:ssl:default": "echo 'support only macOS and Linux'",
    "setup:hosts": "if [[ $(cat /etc/hosts | grep $npm_package_host -c) == 0 ]]; then echo \"127.0.0.1 $npm_package_host\" | sudo tee -a /etc/hosts; fi",
    "setup:mkcert": "run-script-os",
    "setup:mkcert:darwin": "brew install mkcert nss",
    "setup:mkcert:linux": "yarn setup:mkcert:linux:brew && yarn setup:mkcert:linux:install",
    "setup:mkcert:linux:brew": "if command -v yum > /dev/null 2>&1; then sudo yum install nss-tools; else sudo apt install libnss3-tools; fi",
    "setup:mkcert:linux:install": "brew install mkcert nss",
    "setup:cert": "mkcert -install && mkcert $npm_package_host",
    "start": "yarn kill && HOST=$npm_package_host PORT=$npm_package_port NODE_ENV=production yarn ts-node ./server.ts",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 -x next-transpile-modules,framer-motion"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.0",
    "@aspida/swr": "^1.7.0",
    "@auth0/auth0-react": "^1.5.0",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.3",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@fullcalendar/common": "^5.7.0",
    "@fullcalendar/daygrid": "^5.7.0",
    "@fullcalendar/interaction": "^5.7.0",
    "@fullcalendar/react": "^5.7.0",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.0",
    "@technote-space/use-local-storage": "^0.1.7",
    "@technote-space/use-unmount-ref": "^0.1.9",
    "@technote-space/zipcode2address-jp": "^0.1.11",
    "aspida": "^1.7.0",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.22.0",
    "framer-motion": "4.1.11",
    "next": "^10.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.28.1",
    "slick-carousel": "^1.8.1",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.2.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-slick": "^0.23.4",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "class-validator": "^0.13.1",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.11",
    "npm-run-all": "^4.1.5",
    "run-script-os": "^1.1.6",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "4.3"
  },
  "engines": {
    "yarn": "1.x"
  },
  "host": "localhost.frourio-demo.com",
  "port": 3001
}
