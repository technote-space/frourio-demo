{
  "name": "@frourio-demo/admin",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build && next export",
    "dev": "yarn kill && FRONT_URL=$npm_package_front LOCK_URL=$npm_package_lock next dev -p $npm_package_port",
    "kill": "lsof -i:$npm_package_port -t | xargs kill -9",
    "setup": "yarn install --prefer-offline --network-timeout 1000000",
    "start": "yarn kill && FRONT_URL=$npm_package_front LOCK_URL=$npm_package_lock next start -p $npm_package_port",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 -x @date-io/date-fns,next-transpile-modules"
  },
  "dependencies": {
    "@aspida/axios": "^1.6.3",
    "@aspida/swr": "^1.6.3",
    "@date-io/date-fns": "1.3.13",
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@fullcalendar/common": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.11.3",
    "@technote-space/material-table": "^2.3.5",
    "@technote-space/material-table-localization-jp": "^0.1.1",
    "@technote-space/use-local-storage": "^0.1.1",
    "@technote-space/use-material-table-icons": "^0.1.1",
    "@technote-space/use-unmount-ref": "^0.1.2",
    "aspida": "^1.6.3",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "clsx": "^1.1.1",
    "date-fns": "^2.19.0",
    "formik": "^2.2.6",
    "marked": "^2.0.1",
    "next": "^10.0.9",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "sanitize-html": "^2.3.3",
    "swr": "^0.5.4"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.9",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^13.0.6",
    "@types/marked": "^2.0.0",
    "@types/node": "^14.14.35",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/sanitize-html": "^1.27.1",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "class-validator": "^0.13.1",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.11",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "engines": {
    "yarn": "1.x"
  },
  "port": 3000,
  "front": "https://localhost.frourio-demo.com:3001/",
  "lock": "http://localhost:3002/"
}
