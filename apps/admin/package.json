{
  "name": "@frourio-demo/admin",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build && next export",
    "dev": "yarn kill && FRONT_URL=$npm_package_front LOCK_URL=$npm_package_lock next dev -p $npm_package_port",
    "kill": "lsof -i:$npm_package_port -t | xargs kill -9",
    "setup": "yarn install --prefer-offline --network-timeout 1000000",
    "start": "yarn kill && FRONT_URL=$npm_package_front LOCK_URL=$npm_package_lock next start -p $npm_package_port",
    "update": "npm_config_yes=true npx npm-check-updates -u --timeout 100000 -x @date-io/date-fns,next-transpile-modules"
  },
  "dependencies": {
    "@aspida/axios": "^1.7.1",
    "@aspida/swr": "^1.7.1",
    "@date-io/date-fns": "1.3.13",
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@fullcalendar/common": "^5.9.0",
    "@fullcalendar/daygrid": "^5.9.0",
    "@fullcalendar/interaction": "^5.9.0",
    "@fullcalendar/react": "^5.9.0",
    "@material-ui/core": "^4.12.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.3.10",
    "@material-ui/styles": "^4.11.4",
    "@technote-space/material-table": "^2.3.16",
    "@technote-space/material-table-localization-jp": "^0.1.14",
    "@technote-space/use-local-storage": "^0.1.15",
    "@technote-space/use-material-table-icons": "^0.1.12",
    "@technote-space/use-unmount-ref": "^0.1.14",
    "aspida": "^1.7.1",
    "axios": "^0.21.1",
    "chart.js": "^3.5.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.23.0",
    "formik": "^2.2.9",
    "marked": "^2.1.3",
    "next": "^11.0.1",
    "pluralize": "^8.0.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^3.0.4",
    "react-dom": "^17.0.2",
    "sanitize-html": "^2.4.0",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^11.0.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/marked": "^2.0.4",
    "@types/node": "^16.4.7",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/sanitize-html": "^2.3.2",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "class-validator": "^0.13.1",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.1.1",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "ts-loader": "^9.2.4",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "engines": {
    "yarn": "1.x"
  },
  "port": 3000,
  "front": "https://localhost.frourio-demo.com:3001/",
  "lock": "http://localhost:3002/"
}
