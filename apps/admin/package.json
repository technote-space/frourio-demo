{
  "name": "@frourio-demo/admin",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build && next export",
    "dev": "yarn kill && next dev -p $npm_package_port",
    "kill": "lsof -i:$npm_package_port -t | xargs kill -9",
    "license:check": "npx license-checker --excludePrivatePackages --production --onlyAllow 'MIT; Apache-2.0; Apache 2.0; ISC; BSD-3-Clause; BSD-2-Clause; 0BSD; CC-BY-4.0; CC0-1.0; MPL-2.0;'",
    "license:generate": "npx license-checker --excludePrivatePackages --json --customPath ../../license-format.json > ./license.json",
    "setup": "yarn install --prefer-offline --network-timeout 1000000",
    "start": "yarn kill && next start -p $npm_package_port",
    "update": "npx npm-check-updates -u --timeout 100000 -x @date-io/date-fns,next-transpile-modules"
  },
  "dependencies": {
    "@aspida/axios": "^1.3.0",
    "@aspida/swr": "^1.3.0",
    "@date-io/date-fns": "1.3.13",
    "@frourio-demo/types": "*",
    "@frourio-demo/utils": "*",
    "@fullcalendar/common": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.11.3",
    "@technote-space/material-table": "^2.3.0",
    "aspida": "^1.3.0",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "formik": "^2.2.6",
    "next": "^10.0.6",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "swr": "^0.4.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.6",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "@types/node": "^14.14.22",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "class-validator": "^0.13.1",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.7",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "yarn": "1.x"
  },
  "port": 3000
}
