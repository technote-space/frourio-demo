    "null-loader": "^4.0.1",
    "sass": "^1.32.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "yarn": "1.x"
  }
