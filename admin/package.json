    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.11.2",
    "@technote-space/material-table": "^2.3.0",
    "aspida": "^1.2.1",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "class-validator": "^0.13.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "formik": "^2.2.6",
    "next": "^10.0.5",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@testing-library/jest-dom": "^5.11.9",
