    "@testing-library/user-event": "^12.6.2",
    "@types/node": "^12.12.21",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "class-validator": "^0.13.1",
    "dotenv": "^8.2.0",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.6",
    "npm-run-all": "^4.1.5",
