  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "dev": "yarn kill && next dev -p $npm_package_port",
    "kill": "lsof -i:$npm_package_port -t | xargs kill -9",
    "license:check": "npx license-checker --production --onlyAllow 'MIT; Apache-2.0; Apache 2.0; ISC; BSD-3-Clause; BSD-2-Clause; 0BSD; CC-BY-4.0; CC0-1.0; MPL-2.0;'",
    "license:generate": "npx license-checker --json --customPath ../license-format.json > ./license.json",
    "setup": "yarn install --prefer-offline --network-timeout 1000000",
    "start": "yarn kill && next start -p $npm_package_port",
    "update": "npx npm-check-updates -u --timeout 100000 -x next-transpile-modules && yarn install && yarn upgrade && yarn audit"
  },
  "dependencies": {
    "@aspida/axios": "^1.3.0",
    "@aspida/swr": "^1.3.0",
    "@fullcalendar/common": "^5.5.1",
