{
  "compilerOptions": {
    "target": "ESNext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "baseUrl": ".",
    "paths": {
      "$/*": [
        "./*"
      ]
    },
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "rootDir": ".",
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": false
  }
}
