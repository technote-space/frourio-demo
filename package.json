{
  "name": "frourio-demo",
  "version": "0.7.0",
  "private": true,
  "description": "Demo application of Frourio.",
  "keywords": [
    "frourio"
  ],
  "homepage": "https://github.com/technote-space/frourio-demo",
  "bugs": {
    "url": "https://github.com/technote-space/frourio-demo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/frourio-demo.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "workspaces": {
    "packages": [
      "shared/**",
      "apps/**"
    ],
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "analyze:admin": "yarn workspace @frourio-demo/admin analyze",
    "analyze:front": "yarn workspace @frourio-demo/front analyze",
    "build": "yarn license:generate:admin && aspida && run-s build:server build:admin build:front",
    "build:admin": "yarn workspace @frourio-demo/admin build",
    "build:front": "yarn workspace @frourio-demo/front build",
    "build:server": "yarn workspace @frourio-demo/server build",
    "build:types": "aspida && yarn workspace @frourio-demo/server build:types",
    "cover": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --coverage --verbose -i --bail",
    "cover:admin": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --coverage --verbose -i --bail --selectProjects admin",
    "cover:front": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --coverage --verbose -i --bail --selectProjects front",
    "cover:server": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --coverage --verbose --bail --selectProjects server",
    "cover:shared": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --coverage --verbose --bail --selectProjects shared",
    "cover:update": "cross-env NODE_OPTIONS=--max_old_space_size=4096 jest --coverage --verbose -u -i --bail --selectProjects admin",
    "dev": "yarn migrate:dev && run-p dev:*",
    "dev:admin": "yarn workspace @frourio-demo/admin dev",
    "dev:aspida": "aspida --watch",
    "dev:front": "yarn workspace @frourio-demo/front dev",
    "dev:server": "yarn workspace @frourio-demo/server dev",
    "license:check": "run-s license:check:*",
    "license:check:admin": "npx license-checker --excludePrivatePackages --production --start apps/admin --onlyAllow 'MIT; Apache-2.0; Apache 2.0; ISC; BSD-3-Clause; BSD-2-Clause; 0BSD; CC-BY-4.0; CC0-1.0; MPL-2.0;'",
    "license:check:front": "npx license-checker --excludePrivatePackages --production --start apps/front --onlyAllow 'MIT; Apache-2.0; Apache 2.0; ISC; BSD-3-Clause; BSD-2-Clause; 0BSD; CC-BY-4.0; CC0-1.0; MPL-2.0;'",
    "license:check:server": "npx license-checker --excludePrivatePackages --production --start apps/server --onlyAllow 'MIT; Apache-2.0; Apache 2.0; ISC; BSD-3-Clause; BSD-2-Clause; 0BSD; CC-BY-4.0; CC0-1.0; MPL-2.0;'",
    "license:generate": "run-p license:generate:*",
    "license:generate:admin": "npx license-checker --excludePrivatePackages --production --start apps/admin --json --customPath license-format.json > apps/admin/license.json",
    "license:generate:front": "npx license-checker --excludePrivatePackages --production --start apps/front --json --customPath license-format.json > apps/front/license.json",
    "license:generate:server": "npx license-checker --excludePrivatePackages --production --start apps/server --json --customPath license-format.json > apps/server/license.json",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "yarn lint -- --fix",
    "migrate:dev": "yarn workspace @frourio-demo/server migrate:dev",
    "migrate:init": "yarn workspace @frourio-demo/server migrate:init",
    "migrate:reset": "yarn workspace @frourio-demo/server migrate:reset",
    "migrate:seed": "yarn workspace @frourio-demo/server migrate:seed",
    "prepare:cover": "yarn setup:install && yarn setup:config && yarn workspace @frourio-demo/server build:frourio && aspida && yarn license:generate:admin",
    "prepare:eslint": "yarn setup:install",
    "prepare:license": "yarn setup:install",
    "prepare:pages": "< apps/front/package.json jq 'del(.os)' > tmp.json && npx shx cp -u tmp.json apps/front/package.json && yarn setup:install && yarn setup:config && yarn workspace @frourio-demo/server build:frourio && aspida && yarn license:generate:admin",
    "prepare:typecheck": "yarn setup:install && yarn setup:config && yarn license:generate:admin",
    "setup": "yarn setup:install && yarn setup:server && yarn setup:admin && yarn build",
    "setup:admin": "yarn workspace @frourio-demo/admin setup",
    "setup:config": "yarn workspace @frourio-demo/server setup:config",
    "setup:front": "yarn workspace @frourio-demo/front setup",
    "setup:install": "yarn install --ignore-platform --prefer-offline --network-timeout 1000000",
    "setup:server": "yarn workspace @frourio-demo/server setup && yarn migrate:seed",
    "start": "run-p start:*",
    "start:admin": "yarn workspace @frourio-demo/admin start",
    "start:front": "yarn workspace @frourio-demo/front start",
    "start:server": "yarn workspace @frourio-demo/server start",
    "test": "yarn lint && yarn typecheck && yarn cover",
    "typecheck": "yarn build:types && tsc --noEmit -p apps/admin && tsc --noEmit -p apps/front && tsc --noEmit -p apps/server",
    "update": "yarn update:core && yarn workspaces run update && yarn install && yarn upgrade && yarn audit",
    "update:core": "npx npm-check-updates -u --timeout 100000"
  },
  "dependencies": {},
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "aspida": "^1.3.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.19.0",
    "eslint-plugin-aspida": "^0.0.2",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "jest-circus": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "swr": "^0.4.1",
    "ts-jest": "^26.5.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "node": ">=14",
    "yarn": "1.x"
  }
}
