{
  "name": "frourio-demo",
  "version": "0.1.11",
  "description": "Demo application of Frourio.",
  "keywords": [
    "frourio"
  ],
  "homepage": "https://github.com/technote-space/frourio-demo",
  "bugs": {
    "url": "https://github.com/technote-space/frourio-demo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/technote-space/frourio-demo.git"
  },
  "license": "MIT",
  "author": {
    "name": "Technote",
    "email": "technote.space@gmail.com",
    "url": "https://technote.space"
  },
  "scripts": {
    "analyze": "ANALYZE=true next build",
    "build": "run-s license:generate:admin build:server build:admin",
    "build:admin": "aspida && next build && next export",
    "build:electron": "yarn --cwd src/electron setup && yarn --cwd src/electron build && yarn --cwd src/electron dist",
    "build:server": "yarn --cwd server build",
    "build:types": "aspida && yarn --cwd server build:frourio",
    "cover": "jest --coverage --verbose -i --bail",
    "cover:admin": "jest --coverage --verbose -i --bail --selectProjects admin",
    "cover:server": "jest --coverage --verbose --bail --selectProjects server",
    "cover:update": "jest --coverage --verbose -u -i --bail --selectProjects admin",
    "dev": "yarn migrate:dev && run-p dev:*",
    "dev:admin": "next dev -p 3000",
    "dev:aspida": "aspida --watch",
    "dev:server": "yarn --cwd server dev",
    "license:check": "yarn license:check:admin && yarn --cwd server license:check && yarn --cwd src/electron license:check",
    "license:check:admin": "npx license-checker --production --onlyAllow 'MIT; Apache-2.0; Apache 2.0; ISC; BSD-3-Clause; BSD-2-Clause; 0BSD; CC-BY-4.0; CC0-1.0; MPL-2.0;'",
    "license:generate": "yarn license:generate:admin && yarn --cwd server license:generate && yarn --cwd src/electron license:generate",
    "license:generate:admin": "npx license-checker --json --customPath license-format.json > ./license.json",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "yarn lint -- --fix",
    "migrate:dev": "yarn --cwd server migrate:dev",
    "migrate:init": "yarn --cwd server migrate:init",
    "migrate:reset": "yarn --cwd server migrate:reset",
    "migrate:seed": "yarn --cwd server migrate:seed",
    "setup": "yarn install --network-timeout 1000000 && yarn setup:db && yarn build",
    "setup:config": "yarn --cwd server setup:config",
    "setup:db": "yarn --cwd server setup && yarn migrate:seed",
    "setup:install": "yarn install --network-timeout 1000000 && cd server && yarn install --network-timeout 1000000",
    "start": "run-p start:*",
    "start:admin": "next start",
    "start:server": "yarn --cwd server start",
    "test": "yarn lint && yarn build:types && yarn cover",
    "typecheck": "yarn build:types && tsc --noEmit && tsc --noEmit -p server",
    "update": "run-s update:*",
    "update:admin": "npx npm-check-updates -u --timeout 100000 -x @date-io/date-fns,next-transpile-modules && yarn install && yarn upgrade && yarn audit",
    "update:electron": "cd src/electron && npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "update:server": "cd server/prisma && npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "heroku-postbuild": "yarn --cwd server setup:heroku"
  },
  "dependencies": {
    "@aspida/axios": "^1.2.1",
    "@aspida/swr": "^1.2.1",
    "@date-io/date-fns": "1.3.13",
    "@fullcalendar/common": "^5.5.1",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.11.2",
    "@technote-space/material-table": "^2.3.0",
    "aspida": "^1.2.1",
    "axios": "^0.21.1",
    "chart.js": "^2.9.4",
    "class-validator": "^0.13.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "formik": "^2.2.6",
    "next": "^10.0.5",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-dom": "^17.0.1",
    "swr": "^0.4.0"
  },
  "devDependencies": {
    "@jest/types": "^26.6.2",
    "@next/bundle-analyzer": "^10.0.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.21",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.18.0",
    "eslint-plugin-aspida": "^0.0.2",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.0",
    "jest-circus": "^26.6.3",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.5",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "sass": "^1.32.4",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "engines": {
    "yarn": "1.x",
    "node": "15.3.0"
  }
}
