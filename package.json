{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "setup": "run-p setup:*",
    "setup:install": "yarn install && cd server && yarn --cwd server install",
    "setup:db": "yarn --cwd server setup && yarn migrate:seed",
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida && prisma generate --schema server/prisma/schema.prisma && next build && next export",
    "build:server": "npm run build --prefix server",
    "build:types": "aspida && npm run build:frourio --prefix server",
    "analyze": "ANALYZE=true next build",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "migrate:init": "npm run migrate:init --prefix server",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "migrate:seed": "yarn ts-node ./server/prisma/seeds/seeder.ts",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "cover": "jest --coverage",
    "test": "npm run lint && npm run build:types && npm run cover",
    "typecheck": "npm run build:types && tsc --noEmit && tsc --noEmit -p server",
    "update": "yarn update:front && yarn update:server",
    "update:front": "npx npm-check-updates -u --timeout 100000 -x @date-io/date-fns,next-transpile-modules && yarn install && yarn upgrade && yarn audit",
    "update:server": "cd server/prisma && npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit"
  },
  "dependencies": {
    "@aspida/axios": "^1.2.1",
    "@aspida/swr": "^1.2.1",
    "@date-io/date-fns": "1.3.13",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@fullcalendar/daygrid": "^5.5.0",
    "@fullcalendar/interaction": "^5.5.0",
    "@fullcalendar/react": "^5.5.0",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/pickers": "^3.2.10",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "chart.js": "^2.9.4",
    "class-validator": "^0.12.2",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.1",
    "formik": "^2.2.6",
    "framer-motion": "^3.1.4",
    "material-table": "^1.69.2",
    "next": "^10.0.5",
    "pluralize": "^8.0.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^10.0.5",
    "@prisma/cli": "^2.14.0",
    "@testing-library/react": "^11.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/pluralize": "^0.0.29",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "canvas": "^2.6.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.17.0",
    "eslint-plugin-aspida": "^0.0.2",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "next-transpile-modules": "^4.1.0",
    "nock": "^13.0.5",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "sass": "^1.32.2",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
