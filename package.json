{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 3000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "build:client": "aspida && prisma generate --schema server/prisma/schema.prisma && next build && next export",
    "build:server": "npm run build --prefix server",
    "build:types": "aspida && npm run build:frourio --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "migrate:init": "npm run migrate:init --prefix server",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "migrate:seed": "npm run migrate:reset && yarn ts-node ./server/prisma/seeds/seeder.ts",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "cover": "jest --coverage",
    "test": "npm run lint && npm run build:types && jest",
    "typecheck": "npm run build:types && tsc --noEmit && tsc --noEmit -p server",
    "update": "run-s update:*",
    "update:front": "npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit",
    "update:server": "cd server/prisma && npx npm-check-updates -u --timeout 100000 && yarn install && yarn upgrade && yarn audit"
  },
  "dependencies": {
    "@aspida/axios": "^1.1.1",
    "@aspida/swr": "^1.1.1",
    "@chakra-ui/icons": "^1.0.2",
    "@chakra-ui/react": "^1.0.4",
    "@chakra-ui/theme": "^1.2.2",
    "@chakra-ui/theme-tools": "^1.0.2",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.12.2",
    "date-fns": "^2.16.1",
    "dayjs": "^1.9.7",
    "formik": "^2.2.6",
    "framer-motion": "^3.1.1",
    "next": "^10.0.4",
    "react": "^17.0.1",
    "react-cookie": "^4.0.3",
    "react-dom": "^17.0.1",
    "swr": "^0.3.11"
  },
  "devDependencies": {
    "@prisma/cli": "^2.13.1",
    "@testing-library/react": "^11.2.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.16.0",
    "eslint-plugin-react": "^7.21.5",
    "faker": "^5.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "nock": "^13.0.5",
    "npm-run-all": "^4.1.5",
    "sass": "^1.30.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.12",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
